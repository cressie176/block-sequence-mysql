    "husky": "^0.11.4",
    "tap-spec": "^4.1.1"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "debug": "^2.2.0",
    "lodash": "^4.11.1",
    "mysql": "^2.10.2",
    "safe-json-parse": "^4.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/block-sequence-mysql.git"
