{
  "name": "block-sequence-mysql",
  "version": "0.1.3",
  "description": "A mysql implementation of block-sequence",
  "main": "index.js",
  "scripts": {
    "test": "node tests/compliance.tests.js | tap-spec",
    "lint": "eslint .",
